{
  "name": "@gocontractor/gctr-design-system",
  "version": "0.0.3",
  "scripts": {
    "ng": "ng",
    "start": "npm run npm-pack && ng serve",
    "gh": "ng build --prod --base-href \"https://gocontractor.github.io/gctr-design-system/\"",
    "gh-pub": "npx angular-cli-ghpages --dir=dist/GctrDesignSystem",
    "build": "npm run build-lib && npm run docs && ng serve",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "gulp": "gulp",
    "build-lib": "ng build --prod gctr-design-system && cd projects/gctr-design-system && gulp",
    "publish-lib": "cd dist/gctr-design-system && npm publish",
    "npm-pack": "cd dist/gctr-design-system && npm pack",
    "docs": "typedoc --exclude *.scss --exclude *.html --exclude *.spec.ts --mode file --json src/assets/docs.json ./projects/gctr-design-system/src/lib/"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "~9.1.12",
    "@angular/cdk": "~9.2.4",
    "@angular/common": "~9.1.12",
    "@angular/compiler": "~9.1.12",
    "@angular/core": "~9.1.12",
    "@angular/forms": "~9.1.12",
    "@angular/material": "^9.2.4",
    "@angular/platform-browser": "~9.1.12",
    "@angular/platform-browser-dynamic": "~9.1.12",
    "@angular/router": "~9.1.12",
    "@material-extended/mde": "^2.3.1",
    "core-js": "^2.5.4",
    "ngx-markdown": "^11.1.2",
    "rxjs": "~6.5.3",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.12",
    "@angular-devkit/build-ng-packagr": "~0.901.12",
    "@angular/cli": "~9.1.12",
    "@angular/compiler-cli": "^9.1.12",
    "@angular/language-service": "~9.1.12",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "angular-cli-ghpages": "^0.6.0",
    "codelyzer": "^5.1.2",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^6.0.0",
    "gulp-concat": "^2.6.1",
    "gulp-sass": "^4.0.2",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^5.0.7",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^9.0.0",
    "protractor": "^7.0.0",
    "ts-node": "~7.0.0",
    "typedoc": "^0.15.0",
    "typescript": "~3.8.3"
  }
}
