{
  "name": "@gocontractor/gctr-design-system",
  "version": "0.0.3",
  "scripts": {
    "ng": "ng",
    "start": "npm run npm-pack && ng serve",
    "gh": "ng build --prod --base-href \"https://gocontractor.github.io/gctr-design-system/\"",
    "gh-pub": "npx angular-cli-ghpages --dir=dist/GctrDesignSystem",
    "build": "npm run build-lib && npm run docs && ng serve",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "gulp": "gulp",
    "build-lib": "ng build --prod gctr-design-system && cd projects/gctr-design-system && gulp",
    "publish-lib": "cd dist/gctr-design-system && npm publish",
    "npm-pack": "cd dist/gctr-design-system && npm pack",
    "docs": "typedoc --exclude *.scss --exclude *.html --exclude *.spec.ts --mode file --json src/assets/docs.json ./projects/gctr-design-system/src/lib/"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "~10.2.5",
    "@angular/cdk": "~10.2.6",
    "@angular/common": "~10.2.5",
    "@angular/compiler": "~10.2.5",
    "@angular/core": "~10.2.5",
    "@angular/forms": "~10.2.5",
    "@angular/material": "^10.2.7",
    "@angular/platform-browser": "~10.2.5",
    "@angular/platform-browser-dynamic": "~10.2.5",
    "@angular/router": "~10.2.5",
    "@material-extended/mde": "^2.3.1",
    "core-js": "^2.5.4",
    "ngx-markdown": "^10.1.1",
    "rxjs": "~6.5.3",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1002.3",
    "@angular-devkit/build-ng-packagr": "~0.1002.3",
    "@angular/cli": "~10.2.3",
    "@angular/compiler-cli": "^10.2.5",
    "@angular/language-service": "~10.2.5",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "angular-cli-ghpages": "^0.6.0",
    "codelyzer": "^5.1.2",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^6.0.0",
    "gulp-concat": "^2.6.1",
    "gulp-sass": "^4.0.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^10.1.2",
    "protractor": "~7.0.0",
    "ts-node": "~7.0.0",
    "typedoc": "^0.15.0",
    "typescript": "~4.0.7"
  }
}
