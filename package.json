{
  "name": "@gocontractor/gctr-design-system",
  "version": "0.0.3",
  "scripts": {
    "ng": "ng",
    "start": "npm run npm-pack && ng serve",
    "gh": "ng build --prod --base-href \"https://gocontractor.github.io/gctr-design-system/\"",
    "gh-pub": "npx angular-cli-ghpages --dir=dist/GctrDesignSystem",
    "build": "npm run build-lib && npm run docs && ng serve",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "gulp": "gulp",
    "build-lib": "ng build gctr-design-system && cd projects/gctr-design-system && gulp",
    "publish-lib": "cd dist/gctr-design-system && npm publish",
    "npm-pack": "cd dist/gctr-design-system && npm pack",
    "docs": "typedoc --exclude *.scss --exclude *.html --exclude *.spec.ts --mode file --json src/assets/docs.json ./projects/gctr-design-system/src/lib/"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "~8.2.14",
    "@angular/cdk": "~8.2.3",
    "@angular/common": "~8.2.14",
    "@angular/compiler": "~8.2.14",
    "@angular/core": "~8.2.14",
    "@angular/forms": "~8.2.14",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "~8.2.14",
    "@angular/platform-browser-dynamic": "~8.2.14",
    "@angular/router": "~8.2.14",
    "@material-extended/mde": "^2.3.1",
    "core-js": "^2.5.4",
    "ngx-markdown": "^8.2.1",
    "rxjs": "~6.5.3",
    "tslib": "^1.9.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.803.26",
    "@angular-devkit/build-ng-packagr": "~0.803.26",
    "@angular/cli": "~8.3.26",
    "@angular/compiler-cli": "^8.2.14",
    "@angular/language-service": "~8.2.14",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "angular-cli-ghpages": "^0.6.0",
    "codelyzer": "~5.2.2",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^6.0.0",
    "gulp-concat": "^2.6.1",
    "gulp-sass": "^4.0.2",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^5.0.7",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^5.7.0",
    "protractor": "^7.0.0",
    "ts-node": "~7.0.0",
    "tsickle": ">=0.34.0",
    "tslib": "^1.9.0",
    "tslint": "~5.11.0",
    "typedoc": "^0.15.0",
    "typescript": "~3.5.3"
  }
}
